{
	"name": "flashcards",
	"displayName": "Flashcards",
	"description": "Flashcards will set you free",
	"icon": "assets/flashcards.png",
	"version": "0.0.1",
	"publisher": "kim-sardine",
	"engines": {
		"vscode": "^1.44.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"flashcards",
		"flashcard",
		"flash method",
		"index card",
		"card"
	],
    "repository": {
        "type": "git",
        "url": "https://github.com/kim-sardine/flashcards"
	},
	"bugs": {
		"url": "https://github.com/kim-sardine/flashcards/issues",
		"email": "kim-sardine@gmail.com"
	},
	"license": "SEE LICENSE IN LICENSE",
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration": {
			"title": "Flashcards",
			"properties": {
				"flashcards.separator": {
					"type": "string",
					"default": "Tab",
					"enum": [
						"Space",
						"Comma",
						"Double #"
					],
					"enumDescriptions": [
						"Qusetion Answer",
						"Qusetion,Answer",
						"Qusetion##Answer"
					]
				}
			}
		},
		"commands": [
			{
				"command": "flashcards.show-start-modal",
				"title": "Change language",
				"category": "Flashcards"
			},
			{
				"command": "flashcards.change-category",
				"title": "Change category",
				"category": "Flashcards"
			},
			{
				"command": "flashcards.change-display-seconds",
				"title": "Change display seconds",
				"category": "Flashcards"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.46.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.18.0",
		"@typescript-eslint/eslint-plugin": "^2.18.0",
		"glob": "^7.1.6",
		"mocha": "^7.0.1",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0"
	}
}
